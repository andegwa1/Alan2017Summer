    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="24" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.al.alan2017summer.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
                /
            </intent-filter>
        </activity>

        <!-- <activity android:name=".TestActivity"> -->


        <!-- </activity> -->

        <activity android:name="com.example.al.alan2017summer.ViewPagerActivity" />
        <activity
            android:name="com.example.al.alan2017summer.LaunchModeActivity"
            android:launchMode="singleInstance" />
        <activity android:name="com.example.al.alan2017summer.ScaleTypeActivity" />

        <provider
            android:name="com.android.tools.fd.runtime.InstantRunContentProvider"
            android:authorities="com.example.al.alan2017summer.com.android.tools.fd.runtime.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>